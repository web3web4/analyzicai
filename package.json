{
  "name": "uxicai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run supabase:ensure && next dev",
    "build": "pnpm run supabase:ensure && next build",
    "start": "pnpm run supabase:ensure && next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "supabase:ensure": "supabase status >/dev/null 2>&1 || pnpm run supabase:init",
    "supabase:init": "supabase start && supabase db push --local",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:restart": "supabase stop && supabase start",
    "supabase:push": "supabase db push --local",
    "supabase:reset": "echo '\\033[0;31m⚠️  WARNING: This will DESTROY ALL DATA!\\033[0m' && read -p 'Type YES to confirm: ' confirm && [ \"$confirm\" = \"YES\" ] && supabase db reset || echo 'Reset cancelled'",
    "supabase:status": "supabase status",
    "supabase:studio": "supabase studio",
    "supabase:watch": "nodemon --watch supabase/migrations --ext sql --exec 'supabase db push --local'"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "next": "16.1.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@jest/globals": "30.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.3",
    "jest": "30.2.0",
    "supabase": "^2.72.8",
    "tailwindcss": "^4",
    "ts-jest": "29.4.6",
    "typescript": "^5"
  }
}
